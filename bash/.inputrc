set completion-ignore-case On

# Arrow keys match what you've typed so far against your command history
"\e[A": history-search-backward
"\e[B": history-search-forward
"\e[C": forward-char
"\e[D": backward-char

# Bash Keybinds
"\C-fr": " ,radio\nclear\n"         # (R)adio
"\C-fn": " newsboat\nclear\n"       # (N)ews
"\C-fj": " ,twitch\nclear\n"        # Stream (J)ustin
"\C-fm": " neomutt\nclear\n"        # (M)ail
"\C-fc": " ,checkmail\n"            # (C)heck new mail
"\C-fb": " ,pinboard\nclear\n"      # (B)ookmarks

# Bash and Tmux Keybinds
"\C-ff": " ,tmux-session\nclear\n"  # (F)ind projects
"\C-fg": " lazygit\nclear\n"        # Lazy(G)it
"\C-fz": " zk edit -i\nclear\n"     # (Z)ettelkasten
"\C-ft": " ,task\nclear\n"          # (T)ask
